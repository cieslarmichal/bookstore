{
  "name": "bookstore",
  "version": "1.0.0",
  "author": "Michal Cieslar",
  "private": true,
  "scripts": {
    "start": "ts-node src/index.ts",
    "build:clean": "rimraf dist",
    "build": "tsc --build tsconfig.build.json",
    "build:dev": "tsc --build tsconfig.json",
    "test:unit": "jest --config tests-unit.config.js",
    "test:integration": "jest --config tests-integration.config.js --runInBand --detectOpenHandles",
    "test:e2e": "jest --config tests-e2e.config.js --runInBand --detectOpenHandles",
    "test:clean": "jest --clearCache",
    "lint": "eslint . --ext .ts --cache --max-warnings 0",
    "lint:fix": "npm run lint -- --fix"
  },
  "dependencies": {
    "bcrypt": "^5.1.0",
    "bunyan": "^1.8.15",
    "dotenv": "^16.0.1",
    "fastify": "^4.15.0",
    "http-status-codes": "^2.2.0",
    "inversify": "^6.0.1",
    "jsonwebtoken": "^9.0.0",
    "node-fetch": "^2.6.11",
    "pg": "^8.9.0",
    "reflect-metadata": "^0.1.13",
    "tslib": "^2.5.0",
    "typeorm": "^0.3.11",
    "uuid": "^9.0.0",
    "zod": "^3.20.2"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.6.0",
    "@types/bcrypt": "^5.0.0",
    "@types/bunyan": "^1.8.8",
    "@types/jest": "^29.4.0",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/node": "^16.10.0",
    "@types/uuid": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^5.50.0",
    "@typescript-eslint/parser": "^5.50.0",
    "dotenv": "^16.0.3",
    "esbuild": "^0.17.5",
    "eslint": "^8.33.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.5.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.4.2",
    "nodemon": "^2.0.21",
    "prettier": "^2.7.1",
    "rimraf": "^4.1.2",
    "supertest": "^6.2.2",
    "ts-jest": "^29.0.5",
    "ts-loader": "^9.2.6",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.2",
    "typescript": "^4.9.5"
  },
  "volta": {
    "node": "16.10.0"
  }
}
