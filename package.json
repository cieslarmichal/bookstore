{
  "name": "book-store-api",
  "version": "1.0.0",
  "author": "Michal Cieslar",
  "private": true,
  "scripts": {
    "build:clean": "rimraf dist",
    "build": "tsc --build tsconfig.build.json",
    "build:dev": "tsc --build tsconfig.json",
    "test:unit": "vitest .unit.test.ts --threads false",
    "test:unit:watch": "npm run test:unit -- --watch",
    "test:integration": "vitest .integration.test.ts --threads false",
    "test:integration:watch": "npm run test:integration -- --watch",
    "test:watch2": "cross-env NODE_ENV=test jest --watch --runInBand --detectOpenHandles",
    "lint": "eslint . --ext .ts --cache --max-warnings 0",
    "lint:fix": "npm run lint -- --fix"
  },
  "dependencies": {
    "awilix": "^8.0.0",
    "bcrypt": "^5.1.0",
    "bunyan": "^1.8.15",
    "class-validator": "^0.14.0",
    "dotenv": "^16.0.1",
    "express": "^4.18.2",
    "express-async-handler": "^1.2.0",
    "http-status-codes": "^2.2.0",
    "jsonwebtoken": "^9.0.0",
    "reflect-metadata": "^0.1.13",
    "tslib": "^2.5.0",
    "typeorm": "^0.3.11",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.6.0",
    "@types/bcrypt": "^5.0.0",
    "@types/bunyan": "^1.8.8",
    "@types/express": "^4.17.17",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/node": "^18.11.18",
    "@types/supertest": "^2.0.11",
    "@types/uuid": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^5.50.0",
    "@typescript-eslint/parser": "^5.50.0",
    "dotenv": "^16.0.3",
    "esbuild": "^0.17.5",
    "eslint": "^8.33.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.5.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.7.1",
    "rimraf": "^4.1.2",
    "supertest": "^6.2.2",
    "ts-loader": "^9.2.6",
    "ts-node": "^10.5.0",
    "tsconfig-paths": "^4.1.2",
    "typescript": "^4.9.5",
    "vitest": "^0.27.1"
  },
  "volta": {
    "node": "18.11.0"
  },
  "engines": {
    "node": ">=18.0.0 <20",
    "npm": ">=8.0.0"
  }
